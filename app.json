{
  "expo": {
    "name": "JetChat",
    "slug": "JetChat",
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/appstore.png",
    "splash": {
      "image": "./assets/splashJetChat.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/playstore.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.grejojoby.JetChat"
    },
    "web": {
      "favicon": "./assets/playstore.png"
    }
  }
}
